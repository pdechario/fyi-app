{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/fyi-app/src",
  "projects": {
    "fyi-app": {
      "type": "application",
      "root": "apps/fyi-app",
      "entryFile": "main",
      "sourceRoot": "apps/fyi-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fyi-app/tsconfig.app.json"
      }
    },
    "fyi-datapipeline": {
      "type": "library",
      "root": "libs/fyi-datapipeline",
      "entryFile": "index",
      "sourceRoot": "libs/fyi-datapipeline/src",
      "compilerOptions": {
        "tsConfigPath": "libs/fyi-datapipeline/tsconfig.lib.json"
      }
    },
    "fyi-timescale": {
      "type": "library",
      "root": "libs/fyi-timescale",
      "entryFile": "index",
      "sourceRoot": "libs/fyi-timescale/src",
      "compilerOptions": {
        "tsConfigPath": "libs/fyi-timescale/tsconfig.lib.json"
      }
    },
    "webhook-triggers": {
      "type": "library",
      "root": "libs/webhook-triggers",
      "entryFile": "index",
      "sourceRoot": "libs/webhook-triggers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/webhook-triggers/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "tsconfig.build.json",
    "assets": [],
    "watchAssets": true,
    "paths": {
      "@fyi-timescale": [
        "libs/fyi-timescale/src"
      ],
      "@fyi-timescale/*": [
        "libs/fyi-timescale/src/*"
      ]
    }
  }
}