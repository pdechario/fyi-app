{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/fyi-app/src",
  "projects": {
    "fyi-app": {
      "type": "application",
      "root": "apps/fyi-app",
      "entryFile": "main",
      "sourceRoot": "apps/fyi-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fyi-app/tsconfig.app.json"
      }
    },
    "fyi-timescale": {
      "type": "library",
      "root": "libs/fyi-timescale",
      "entryFile": "index",
      "sourceRoot": "libs/fyi-timescale/src",
      "compilerOptions": {
        "tsConfigPath": "libs/fyi-timescale/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions":{
    "webpack":true,
    "tsConfigPath": "tsconfig.build.json",
    "assets": [],
    "watchAssets": true,
    "paths":{
        "@fyi-timescale": ["libs/fyi-timescale/src"],
        "@fyi-timescale/*": ["libs/fyi-timescale/src/*"] 
    }
  }
}