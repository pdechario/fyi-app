{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "sourceRoot": "apps/fyi-app/src",
  "projects": {
    "fyi-app": {
      "type": "application",
      "root": "apps/fyi-app",
      "entryFile": "main",
      "sourceRoot": "apps/fyi-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fyi-app/tsconfig.app.json"
      }
    },
    "timescale": {
      "type": "library",
      "root": "libs/timescale",
      "entryFile": "index",
      "sourceRoot": "libs/timescale/src",
      "compilerOptions": {
        "tsConfigPath": "libs/timescale/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}