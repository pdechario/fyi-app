{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@app/webhook-triggers": [
        "libs/webhook-triggers/src"
      ],
      "@app/webhook-triggers/*": [
        "libs/webhook-triggers/src/*"
      ],
      "@fyi-timescale": [
        "libs/fyi-timescale/src"
      ],
      "@fyi-timescale/*": [
        "libs/fyi-timescale/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}